#. Tag: varname
#: mobilitydb-manual.xml:1956
#, no-c-format
msgid "intersectsPeriodSet"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1957
#, no-c-format
msgid "Does the temporal value intersect the period set?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1958
#, no-c-format
msgid "intersectsPeriodSet(ttype, periodset): boolean"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:1959
#, no-c-format
msgid ""
"SELECT intersectsPeriodSet(tbool '[t@2012-01-01, f@2012-01-15]',\n"
"        periodset '{[2012-01-01, 2012-01-03), [2012-01-05, 2012-01-07)}');\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1963
#, no-c-format
msgid "twAvg"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1964
#, no-c-format
msgid "Get the time-weighted average"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1965
#, no-c-format
msgid "twAvg(tnumber): float"
msgstr ""

