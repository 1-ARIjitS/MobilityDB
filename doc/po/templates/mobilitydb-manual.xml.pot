#, no-c-format
msgid "&amp;&amp;"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:669
#, no-c-format
msgid "Do the time values overlap (have points in common)?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:670
#, no-c-format
msgid ""
"{timestampset, period, periodset} &amp;&amp; {timestampset, period, "
"periodset}"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:671
#, no-c-format
msgid ""
"SELECT period '[2011-01-01, 2011-01-05)' &amp;&amp; period '[2011-01-02, "
"2011-01-07)';\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:675 mobilitydb-manual.xml:1221
#, no-c-format
msgid "@&gt;"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:676
#, no-c-format
msgid "Does the first time value contain the second one?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:677
#, no-c-format
