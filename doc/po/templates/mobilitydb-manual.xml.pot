msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:647 mobilitydb-manual.xml:1197
#: mobilitydb-manual.xml:2580
#, no-c-format
msgid "<varname>*</varname>"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:648
#, no-c-format
msgid "Intersection of the time values"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:649
#, no-c-format
msgid "time * time"
msgstr ""

