#. Tag: title
#: mobilitydb-manual.xml:1972 mobilitydb-manual.xml:3980
#, no-c-format
msgid "Spatial Functions"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1973
#, no-c-format
msgid ""
"In the following, we specify with the symbol &Z_support; that the function "
"supports 3D points and with the symbol &geography_support; that the function "
"is available for geographies."
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1977
#, no-c-format
msgid "asText"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1978
#, no-c-format
msgid ""
"Get the Well-Known Text (WKT) representation &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1979
#, no-c-format
msgid "asText({tpoint, tpoint[], geo[]}): {text, text[]}"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:1980
#, no-c-format
msgid ""
"SELECT asText(tgeompoint 'SRID=4326;[Point(0 0 0)@2012-01-01, Point(1 1 "
"1)@2012-01-02)');\n"
"-- \"[POINT Z (0 0 0)@2012-01-01 00:00:00+00, POINT Z (1 1 1)@2012-01-02 "
"00:00:00+00)\"\n"
"SELECT asText(ARRAY[geometry 'Point(0 0)', 'Point(1 1)']);\n"
"-- \"{\"POINT(0 0)\",\"POINT(1 1)\"}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1984
#, no-c-format
msgid "asEWKT"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1985
#, no-c-format
msgid ""
"Get the Extended Well-Known Text (EWKT) representation &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1986
#, no-c-format
msgid "asEWKT({tpoint, tpoint[], geo[]}): {text, text[]}"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:1987
#, no-c-format
msgid ""
"SELECT asEWKT(tgeompoint 'SRID=4326;[Point(0 0 0)@2012-01-01, Point(1 1 "
"1)@2012-01-02)');\n"
"-- \"SRID=4326;[POINT Z (0 0 0)@2012-01-01 00:00:00+00, \n"
"        POINT Z (1 1 1)@2012-01-02 00:00:00+00)\"\n"
"SELECT asEWKT(ARRAY[geometry 'SRID=5676;Point(0 0)', 'SRID=5676;Point(1 "
"1)']);\n"
"-- \"{\"SRID=5676;POINT(0 0)\",\"SRID=5676;POINT(1 1)\"}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1991
#, no-c-format
msgid "asMFJSON"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1992
#, no-c-format
msgid ""
"Get the Moving Features JSON representation &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1993
#, no-c-format
msgid ""
"asMFJSON(tpoint, maxdecdigits int4 DEFAULT 15, options int4 DEFAULT 0): bytea"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1994
#, no-c-format
msgid ""
"The last <varname>options</varname> argument could be used to add BBOX and/"
"or CRS in MFJSON output:"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1996
#, no-c-format
msgid "0: means no option (default value)"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1997
#, no-c-format
msgid "1: MFJSON BBOX"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1998
#, no-c-format
msgid "2: MFJSON Short CRS (e.g EPSG:4326)"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1999
#, no-c-format
msgid "4: MFJSON Long CRS (e.g urn:ogc:def:crs:EPSG::4326)"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2001
#, no-c-format
msgid ""
"SELECT asMFJSON(tgeompoint 'Point(1 2)@2019-01-01 18:00:00.15+02');\n"
"-- \"{\"type\":\"MovingPoint\",\"coordinates\":[1,2],\"datetimes\":"
"\"2019-01-01T17:00:00.15+01\",\n"
"        \"interpolations\":[\"Discrete\"]}\"\n"
"SELECT asMFJSON(tgeompoint 'SRID=4326;\n"
"        Point(50.813810 4.384260)@2019-01-01 18:00:00.15+02', 2, 3);\n"
"-- \"{\"type\":\"MovingPoint\",\"crs\":{\"type\":\"name\",\"properties\":"
"{\"name\":\"EPSG:4326\"}},\n"
"        \"stBoundedBy\":{\"bbox\":[50.81,4.38,50.81,4.38],\n"
"        \"period\":{\"begin\":\"2019-01-01 17:00:00.15+01\",\"end\":"
"\"2019-01-01 17:00:00.15+01\"}},\n"
"        \"coordinates\":[50.81,4.38],\"datetimes\":"
"\"2019-01-01T17:00:00.15+01\",\n"
"        \"interpolations\":[\"Discrete\"]}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2005
#, no-c-format
msgid "asBinary"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2006
#, no-c-format
msgid ""
"Get the Well-Known Binary (WKB) representation &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2007
#, no-c-format
msgid "asBinary(tpoint): bytea"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2008
#, no-c-format
msgid "asBinary(tpoint, text): bytea"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2009 mobilitydb-manual.xml:2018
#, no-c-format
msgid ""
"The result is encoded using either the little-endian (NDR) or the big-endian "
"(XDR) encoding. If no encoding is specified, then the encoding of the "
"machine is used."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2010
#, no-c-format
msgid ""
"SELECT asBinary(tgeompoint 'SRID=4326;Point(1 2 3)@2012-01-01');\n"
"-- \"\\001\\001\\000\\000\\000\\000\\000\\000\\360?"
"\\000\\000\\000\\000\\000\\000\\000@\\000\\000\\000\\000\\000\\000\\010@"
"\\000\\374\\340\\023jX\\001\\000\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2014
#, no-c-format
msgid "asEWKB"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2015
#, no-c-format
msgid ""
"Get the Extended Well-Known Binary (EWKB) representation &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2016
#, no-c-format
msgid "asEWKB(tpoint): bytea"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2017
#, no-c-format
msgid "asEWKB(tpoint, text): bytea"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2019
#, no-c-format
msgid ""
"SELECT asEWKB(tgeompoint 'SRID=4326;Point(1 2 3)@2012-01-01');\n"
"-- \"\\0011\\346\\020\\000\\000\\000\\000\\000\\000\\000\\000\\360?"
"\\000\\000\\000\\000\\000\\000\\000@\\000\\000\\000\\000\\000\\000\\010@"
"\\000\\374\\340\\023jX\\001\\000\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2023
#, no-c-format
msgid "asHexEWKB"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2024
#, no-c-format
msgid ""
"Get the Hexadecimal Extended Well-Known Binary (EWKB) representation as text "
"&Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2025
#, no-c-format
msgid "asHexEWKB(tpoint): text"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2026
#, no-c-format
msgid "asHexEWKB(tpoint, text): text"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2027
#, no-c-format
msgid ""
"The result is encoded using either the little-endian (NDR) or the big-endian "
"(XDR) encoding. If no encoding is specified, then NDR is used."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2028
#, no-c-format
msgid ""
"SELECT asHexEWKB(tgeompoint 'SRID=4326;Point(1 2 3)@2012-01-01');\n"
"-- "
"\"0131E6100000000000000000F03F0000000000000040000000000000084000FCE0136A580100\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2032
#, no-c-format
msgid "fromMFJSON"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2033
#, no-c-format
msgid ""
"Input a temporal point from a Moving Features JSON representation "
"&Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2034
#, no-c-format
msgid "fromMFJSON(text): tpoint"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2035
#, no-c-format
msgid ""
"SELECT asEWKT(fromMFJSON(text '{\"type\":\"MovingPoint\",\"crs\":{\"type\":"
"\"name\",\n"
"\"properties\":{\"name\":\"EPSG:4326\"}},\"coordinates\":[50.81,4.38],\n"
"\"datetimes\":\"2019-01-01T17:00:00.15+01\",\"interpolations\":[\"Discrete"
"\"]}'));\n"
"-- \"SRID=4326;POINT(50.81 4.38)@2019-01-01 17:00:00.15+01\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2039
#, no-c-format
msgid "fromEWKB"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2040
#, no-c-format
msgid ""
"Input a temporal point from an Extended Well-Known Binary (EWKB) "
"representation &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2041
#, no-c-format
msgid "fromEWKB(bytea): tpoint"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2042
#, no-c-format
msgid ""
"SELECT asEWKT(fromEWKB(bytea \n"
"'\\0011\\346\\020\\000\\000\\000\\000\\000\\000\\000\\000\\360?"
"\\000\\000\\000\\000\\000\\000\\000@\\000\\000\\000\\000\\000\\000\\010@"
"\\000\\374\\340\\023jX\\001\\000'));\n"
"-- \"SRID=4326;POINT Z (1 2 3)@2012-01-01 00:00:00+01\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2047
#, no-c-format
msgid "Get the spatial reference identifier &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2048
#, no-c-format
msgid "SRID(tpoint): integer"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2049
#, no-c-format
msgid ""
"SELECT SRID(tgeompoint 'Point(0 0)@2012-01-01');\n"
"-- 0"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2054
#, no-c-format
msgid "Set the spatial reference identifier &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2055
#, no-c-format
msgid "setSRID(tpoint): tpoint"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2056
#, no-c-format
msgid ""
"SELECT asEWKT(setSRID(tgeompoint '[Point(0 0)@2012-01-01, Point(1 "
"1)@2012-01-02)', 4326));\n"
"-- \"SRID=4326;[POINT(0 0)@2012-01-01 00:00:00+00, POINT(1 1)@2012-01-02 "
"00:00:00+00)\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2061
#, no-c-format
msgid ""
"Transform to a different spatial reference &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2062
#, no-c-format
msgid "transform(tpoint, integer): tpoint"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2063
#, no-c-format
msgid ""
"SELECT asEWKT(transform(tgeompoint 'SRID=4326;Point(4.35 50.85)@2012-01-01', "
"3812));\n"
"-- \"SRID=3812;POINT(648679.018035303 671067.055638114)@2012-01-01 "
"00:00:00+00\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2068
#, no-c-format
msgid ""
"Round the coordinate values to a number of decimal places &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2069
#, no-c-format
msgid "setPrecision(tpoint, int): tpoint"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2070
#, no-c-format
msgid ""
"SELECT asText(setPrecision(tgeompoint '{Point(1.12345 1.12345 "
"1.12345)@2000-01-01, \n"
"        Point(2 2 2)@2000-01-02, Point(1.12345 1.12345 "
"1.12345)@2000-01-03}', 2));\n"
"-- \"{POINT Z (1.12 1.12 1.12)@2000-01-01, POINT Z (2 2 2)@2000-01-02, \n"
"        POINT Z (1.12 1.12 1.12)@2000-01-03}\"\n"
"SELECT asText(setPrecision(tgeogpoint 'Point(1.12345 1.12345)@2000-01-01', "
"2));\n"
"-- \"POINT(1.12 1.12)@2000-01-01\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2074
#, no-c-format
msgid "length"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2075
#, no-c-format
msgid ""
"Get the length traversed by the temporal point &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2076
#, no-c-format
msgid "length(tpoint): float"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2077
#, no-c-format
msgid ""
"SELECT length(tgeompoint '[Point(0 0 0)@2000-01-01, Point(1 1 "
"1)@2000-01-02]');\n"
"-- 1.73205080756888\n"
"SELECT length(tgeompoint '[Point(0 0 0)@2000-01-01, Point(1 1 "
"1)@2000-01-02,\n"
"        Point(0 0 0)@2000-01-03]');\n"
"-- 3.46410161513775\n"
"SELECT length(tgeompoint 'Interp=Stepwise;[Point(0 0 0)@2000-01-01, Point(1 "
"1 1)@2000-01-02,\n"
"        Point(0 0 0)@2000-01-03]');\n"
"-- 0"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2081
#, no-c-format
msgid "cumulativeLength"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2082
#, no-c-format
msgid ""
"Get the cumulative length traversed by the temporal point &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2083
#, no-c-format
msgid "cumulativeLength(tpoint): tfloatseq"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2084
#, no-c-format
msgid ""
"SELECT cumulativeLength(tgeompoint\n"
"        '{[Point(0 0)@2000-01-01, Point(1 1)@2000-01-02, Point(1 "
"0)@2000-01-03],\n"
"        [Point(1 0)@2000-01-04, Point(0 0)@2000-01-05]}');\n"
"-- \"{[0@2000-01-01, 1.4142135623731@2000-01-02, "
"2.41421356237309@2000-01-03],\n"
"        [2.41421356237309@2000-01-04, 3.41421356237309@2000-01-05]}\"\n"
"SELECT cumulativeLength(tgeompoint 'Interp=Stepwise;[Point(0 0 "
"0)@2000-01-01, Point(1 1 1)@2000-01-02,\n"
"        Point(0 0 0)@2000-01-03]');\n"
"-- \"Interp=Stepwise;[0@2000-01-01, 0@2000-01-02, 0@2000-01-03]\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2088
#, no-c-format
msgid "speed"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2089
#, no-c-format
msgid ""
"Get the speed of the temporal point in units per second &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2090
#, no-c-format
msgid "speed(tpoint): tfloats"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2091
#, no-c-format
msgid ""
"SELECT speed(tgeompoint '{[Point(0 0)@2000-01-01, Point(1 1)@2000-01-02, "
"Point(1 0)@2000-01-03],\n"
"        [Point(1 0)@2000-01-04, Point(0 0)@2000-01-05]}') * 3600 * 24;\n"
"-- \"Interp=Stepwise;{[1.4142135623731@2000-01-01, 1@2000-01-02, "
"1@2000-01-03], [1@2000-01-04, 1@2000-01-05]}\"\n"
"SELECT speed(tgeompoint 'Interp=Stepwise;[Point(0 0)@2000-01-01, Point(1 "
"1)@2000-01-02, Point(1 0)@2000-01-03]') * 3600 * 24;\n"
"-- \"Interp=Stepwise;[0@2000-01-01 00:00:00+01, 0@2000-01-03 00:00:00+01]\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2095
#, no-c-format
msgid "twCentroid"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2096
#, no-c-format
msgid "Get the time-weighted centroid &Z_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2097
#, no-c-format
msgid "twCentroid(tgeompoint): point"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2098
#, no-c-format
msgid ""
"SELECT ST_AsText(twCentroid(tgeompoint '{[Point(0 0 0)@2012-01-01, \n"
"        Point(0 1 1)@2012-01-02, Point(0 1 1)@2012-01-03, Point(0 0 "
"0)@2012-01-04)}'));\n"
"-- \"POINT Z (0 0.666666666666667 0.666666666666667)\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2102
#, no-c-format
msgid "azimuth"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2103
#, no-c-format
msgid "Get the temporal azimuth &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2104
#, no-c-format
msgid "azimuth(tpoint): tfloat"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2105
#, no-c-format
msgid ""
"SELECT degrees(azimuth(tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 "
"1)@2012-01-02,\n"
"        Point(1 1 1)@2012-01-03, Point(0 0 0)@2012-01-04)'));\n"
"-- \"Interp=Stepwise;{[45@2012-01-01, 45@2012-01-02], [225@2012-01-03, "
"225@2012-01-04)}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2109
#, no-c-format
msgid "nearestApproachInstant"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2110
#, no-c-format
msgid ""
"Get the instant of the first temporal point at which the two arguments are "
"at the nearest distance &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2111
#, no-c-format
msgid "nearestApproachInstant({geo, tpoint}, {geo, tpoint}): tpoint"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2112
#, no-c-format
msgid ""
"The function will only return the first instant that it finds if there are "
"more than one. The resulting instant may be at an exclusive bound."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2113
#, no-c-format
msgid ""
"SELECT asText(NearestApproachInstant(tgeompoint '(Point(1 1)@2000-01-01, \n"
"        Point(3 1)@2000-01-03]', geometry 'Linestring(1 3,2 2,3 3)'));\n"
"-- \"POINT(2 1)@2000-01-02\"\n"
"SELECT asText(NearestApproachInstant(tgeompoint 'Interp=Stepwise;(Point(1 "
"1)@2000-01-01,\n"
"        Point(3 1)@2000-01-03]', geometry 'Linestring(1 3,2 2,3 3)'));\n"
"-- \"POINT(1 1)@2000-01-01\"\n"
"SELECT asText(NearestApproachInstant(tgeompoint '(Point(1 1)@2000-01-01,\n"
"        Point(2 2)@2000-01-03]', tgeompoint '(Point(1 1)@2000-01-01, Point(4 "
"1)@2000-01-03]'));\n"
"-- \"POINT(1 1)@2000-01-01\"\n"
"SELECT asText(nearestApproachInstant(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03,\n"
"        Point(0 0 0)@2012-01-05)', tgeompoint '[Point(2 0 0)@2012-01-02,\n"
"        Point(1 1 1)@2012-01-04, Point(2 2 2)@2012-01-06)'));\n"
"-- \"POINT Z (0.75 0.75 0.75)@2012-01-03 12:00:00+00\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2114
#, no-c-format
msgid ""
"Function <varname>nearestApproachInstant</varname> generalizes the PostGIS "
"function <varname>ST_ClosestPointOfApproach</varname>. First, the latter "
"function requires both arguments to be trajectories. Second, function "
"<varname>nearestApproachInstant</varname> returns both the point and the "
"timestamp of the nearest point of approach while the PostGIS function only "
"provides the timestamp as shown next."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2115
#, no-c-format
msgid ""
"SELECT to_timestamp(ST_ClosestPointOfApproach(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03, \n"
"                Point(0 0 0)@2012-01-05)'::geometry,\n"
"        tgeompoint '[Point(2 0 0)@2012-01-02, Point(1 1 1)@2012-01-04, \n"
"                Point(2 2 2)@2012-01-06)'::geometry));\n"
"-- \"2012-01-03 12:00:00+00\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2119
#, no-c-format
msgid "nearestApproachDistance"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2120 mobilitydb-manual.xml:2686
#, no-c-format
msgid "Get the smallest distance ever &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2121
#, no-c-format
msgid "nearestApproachDistance({geo, tpoint}, {geo, tpoint}): float"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2122
#, no-c-format
msgid ""
"SELECT NearestApproachDistance(tgeompoint '(Point(1 1)@2000-01-01, \n"
"        Point(3 1)@2000-01-03]', geometry 'Linestring(1 3,2 2,3 3)');\n"
"-- 1\n"
"SELECT NearestApproachDistance(tgeompoint 'Interp=Stepwise;(Point(1 "
"1)@2000-01-01,\n"
"        Point(3 1)@2000-01-03]', geometry 'Linestring(1 3,2 2,3 3)');\n"
"-- 1.4142135623731\n"
"SELECT nearestApproachDistance(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03, \n"
"                Point(0 0 0)@2012-01-05)',\n"
"        tgeompoint '[Point(2 0 0)@2012-01-02, Point(1 1 1)@2012-01-04, \n"
"                Point(2 2 2)@2012-01-06)');\n"
"-- \"0.5\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2123
#, no-c-format
msgid ""
"Function <varname>nearestApproachDistance</varname> has an associated "
"operator <varname>|=|</varname> that can be used for doing nearest neightbor "
"searches using a GiST index (see <xref linkend=\"operators_temporal_types\"/"
">). This function corresponds to the function <varname>ST_DistanceCPA</"
"varname> provided by PostGIS, altough the latter requires both arguments to "
"be a trajectory."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2124
#, no-c-format
msgid ""
"SELECT ST_DistanceCPA(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03, \n"
"                Point(0 0 0)@2012-01-05)'::geometry,\n"
"        tgeompoint '[Point(2 0 0)@2012-01-02, Point(1 1 1)@2012-01-04, \n"
"                Point(2 2 2)@2012-01-06)'::geometry);\n"
"-- \"0.5\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2128
#, no-c-format
msgid "shortestLine"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2129
#, no-c-format
msgid ""
"Get the line connecting the nearest approach point &Z_support; "
"&geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2130
#, no-c-format
msgid "shortestLine({geo, tpoint}, {geo, tpoint}): geo"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2131
#, no-c-format
msgid ""
"The function will only return the first line that it finds if there are more "
"than one."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2132
#, no-c-format
msgid ""
"SELECT ST_AsText(shortestLine(tgeompoint '(Point(1 1)@2000-01-01, \n"
"        Point(3 1)@2000-01-03]', geometry 'Linestring(1 3,2 2,3 3)'));\n"
"-- \"LINESTRING(2 1,2 2)\"\n"
"SELECT ST_AsText(shortestLine(tgeompoint 'Interp=Stepwise;(Point(1 "
"1)@2000-01-01,\n"
"        Point(3 1)@2000-01-03]', geometry 'Linestring(1 3,2 2,3 3)'));\n"
"-- \"LINESTRING(1 1,2 2)\"\n"
"SELECT ST_AsText(shortestLine(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03, \n"
"                Point(0 0 0)@2012-01-05)',\n"
"        tgeompoint '[Point(2 0 0)@2012-01-02, Point(1 1 1)@2012-01-04, \n"
"                Point(2 2 2)@2012-01-06)'));\n"
"-- \"LINESTRING Z (0.75 0.75 0.75,1.25 0.75 0.75)\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2133
#, no-c-format
msgid ""
"Function <varname>shortestLine</varname> can be used to obtain the result "
"provided by the PostGIS function <varname>ST_CPAWithin</varname> when both "
"arguments are trajectories as shown next."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2134
#, no-c-format
msgid ""
"SELECT ST_Length(shortestLine(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03, \n"
"                Point(0 0 0)@2012-01-05)',\n"
"        tgeompoint '[Point(2 0 0)@2012-01-02, Point(1 1 1)@2012-01-04, \n"
"                Point(2 2 2)@2012-01-06)')) &lt;= 0.5;\n"
"-- true\n"
"SELECT ST_CPAWithin(\n"
"        tgeompoint '[Point(0 0 0)@2012-01-01, Point(1 1 1)@2012-01-03, \n"
"                Point(0 0 0)@2012-01-05)'::geometry,\n"
"        tgeompoint '[Point(2 0 0)@2012-01-02, Point(1 1 1)@2012-01-04, \n"
"                Point(2 2 2)@2012-01-06)'::geometry, 0.5);\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2138
#, no-c-format
msgid "simplify"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2139
#, no-c-format
msgid ""
"Simplify a temporal point using a generalization of the Douglas-Peucker "
"algorithm &Z_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2140
#, no-c-format
msgid "simplify(tpoint, float): tpoint"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2141
#, no-c-format
msgid "simplify(tpoint, float, float): tpoint"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2142
#, no-c-format
msgid ""
"The first version remove points that are less than the distance passed as "
"second argument, which is specified in the units of the coordinate system. "
"The second version remove points that are less than the distance passed as "
"second argument provided that the speed difference between the point and the "
"corresponding point in the simplified version is less than the speed passed "
"as third argument, which is specified in units per second. Notice that "
"simplification applies only to temporal sequences or sequence sets with "
"linear interpolation. In all other cases, a copy of the given temporal point "
"is returned."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2144
#, no-c-format
msgid ""
"-- Only distance specified\n"
"SELECT ST_AsText(trajectory(simplify(tgeompoint '[Point(0 4)@2000-01-01,\n"
"        Point(1 1)@2000-01-02, Point(2 3)@2000-01-03, Point(3 "
"1)@2000-01-04,\n"
"        Point(4 3)@2000-01-05, Point(5 0)@2000-01-06, Point(6 "
"4)@2000-01-07]', 1.5)));\n"
"-- \"LINESTRING(0 4,1 1,4 3,5 0,6 4)\"\n"
"SELECT ST_AsText(trajectory(simplify(tgeompoint '[Point(0 4)@2000-01-01,\n"
"        Point(1 1)@2000-01-02, Point(2 3)@2000-01-03, Point(3 "
"1)@2000-01-04,\n"
"        Point(4 3)@2000-01-05, Point(5 0)@2000-01-06, Point(6 "
"4)@2000-01-07]', 2)));\n"
"-- \"LINESTRING(0 4,5 0,6 4)\"\n"
"SELECT ST_AsText(trajectory(simplify(tgeompoint '[Point(0 4)@2000-01-01,\n"
"        Point(1 1)@2000-01-02, Point(2 3)@2000-01-03, Point(3 "
"1)@2000-01-04,\n"
"        Point(4 3)@2000-01-05, Point(5 0)@2000-01-06, Point(6 "
"4)@2000-01-07]', 4)));\n"
"-- \"LINESTRING(0 4,6 4)\"\n"
"\n"
"-- Speed of the temporal point\n"
"SELECT round(speed(tgeompoint '[Point(0 4)@2000-01-01, Point(1 "
"1)@2000-01-02,\n"
"Point(2 3)@2000-01-03, Point(3 1)@2000-01-04, Point(4 3)@2000-01-05,\n"
"Point(5 0)@2000-01-06, Point(6 4)@2000-01-07]') * 1e5, 2);\n"
"-- \"Interp=Stepwise;[3.66@2000-01-01, 2.59@2000-01-02, 3.66@2000-01-05,\n"
"        4.77@2000-01-06, 4.77@2000-01-07]\"\n"
"\n"
"-- Both distance and delta speed specified\n"
"SELECT ST_AsText(trajectory(simplify(tgeompoint '[Point(0 4)@2000-01-01,\n"
"        Point(1 1)@2000-01-02, Point(2 3)@2000-01-03, Point(3 "
"1)@2000-01-04,\n"
"        Point(4 3)@2000-01-05, Point(5 0)@2000-01-06, Point(6 "
"4)@2000-01-07]', 4, 1 / 1e5)));\n"
"-- \"LINESTRING(0 4,1 1,2 3,3 1,4 3,5 0,6 4)\"\n"
"SELECT ST_AsText(trajectory(simplify(tgeompoint '[Point(0 4)@2000-01-01,\n"
"        Point(1 1)@2000-01-02, Point(2 3)@2000-01-03, Point(3 "
"1)@2000-01-04,\n"
"        Point(4 3)@2000-01-05, Point(5 0)@2000-01-06, Point(6 "
"4)@2000-01-07]', 4, 2 / 1e5)));\n"
"-- \"LINESTRING(0 4,1 1,5 0,6 4)\"\n"
"SELECT ST_AsText(trajectory(simplify(tgeompoint '[Point(0 4)@2000-01-01,\n"
"        Point(1 1)@2000-01-02, Point(2 3)@2000-01-03, Point(3 "
"1)@2000-01-04,\n"
"        Point(4 3)@2000-01-05, Point(5 0)@2000-01-06, Point(6 "
"4)@2000-01-07]', 4, 3 / 1e5)));\n"
"-- \"LINESTRING(0 4,6 4)\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2145
#, no-c-format
msgid ""
"A typical use for the <varname>simplify</varname> function is to reduce the "
"size of a dataset, in particular for visualization purposes."
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2149
#, no-c-format
msgid "geoMeasure"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2150
#, no-c-format
msgid ""
"Construct a geometry/geography with M measure from a temporal point and a "
"temporal float &Z_support; &geography_support;"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2151
#, no-c-format
msgid "geoMeasure(tpoint, tfloat, segmentize = false): geo"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2152
#, no-c-format
msgid ""
"The last <varname>segmentize</varname> argument states whether a resulting "
"Linestring M value is transformed into a MultiLinestring M, where each "
"component is a segment of two points."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2154
#, no-c-format
msgid ""
"SELECT st_astext(geoMeasure(tgeompoint '{Point(1 1 1)@2000-01-01,\n"
"        Point(2 2 2)@2000-01-02}', '{5@2000-01-01, 5@2000-01-02}'));\n"
"-- \"MULTIPOINT ZM (1 1 1 5,2 2 2 5)\"\n"
"SELECT st_astext(geoMeasure(tgeogpoint '{[Point(1 1)@2000-01-01, Point(2 "
"2)@2000-01-02],\n"
"        [Point(1 1)@2000-01-03, Point(1 1)@2000-01-04]}',\n"
"        '{[5@2000-01-01, 5@2000-01-02],[7@2000-01-03, 7@2000-01-04]}'));\n"
"-- \"GEOMETRYCOLLECTION M (LINESTRING M (1 1 5,2 2 5),POINT M (1 1 7))\"\n"
"SELECT st_astext(geoMeasure(tgeompoint '[Point(1 1)@2000-01-01,\n"
"        Point(2 2)@2000-01-02, Point(1 1)@2000-01-03]', '[5@2000-01-01, "
"7@2000-01-02, 5@2000-01-03]', true));\n"
"-- \"MULTILINESTRING M ((1 1 5,2 2 5),(2 2 7,1 1 7))\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2155
#, no-c-format
msgid ""
"A typical visualization for mobility data is to show on a map the trajectory "
"of the moving object using different colors according to the speed. <xref "
"linkend=\"figspeed\"/> shows the result of the query below using a color "
"ramp in QGIS."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2156
#, no-c-format
msgid ""
"WITH Temp(t) AS (\n"
"        SELECT tgeompoint '[Point(0 0)@2012-01-01, Point(1 1)@2012-01-05,\n"
"                        Point(2 0)@2012-01-08, Point(3 1)@2012-01-10, "
"Point(4 0)@2012-01-11]'\n"
")\n"
"SELECT ST_AsText(geoMeasure(t,  round(speed(t) * 3600 * 24, 2), true))\n"
"FROM Temp;\n"
"-- \"MULTILINESTRING M ((0 0 0.35,1 1 0.35),(1 1 0.47,2 0 0.47),(2 0 0.71,3 "
"1 0.71),\n"
"        (3 1 1.41,4 0 1.41))\""
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2157
#, no-c-format
msgid ""
"The following expression is used in QGIS to achieve this. The "
"<varname>scale_linear</varname> function transforms the M value of each "
"composing segment to the range [0, 1]. This value is then passed to the "
"<varname>ramp_color</varname> function."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2158
#, no-c-format
msgid ""
"ramp_color(\n"
"        'RdYlBu',\n"
"        scale_linear(\n"
"                m(start_point(geometry_n($geometry,@geometry_part_num))),\n"
"                0, 2, 0, 1\n"
"        )\n"
")"
msgstr ""

#. Tag: title
#: mobilitydb-manual.xml:2159
#, no-c-format
msgid "Visualizing the speed of a moving object using a color ramp in QGIS."
msgstr ""

#. Tag: title
#: mobilitydb-manual.xml:2170 mobilitydb-manual.xml:4074
#, no-c-format
msgid "Restriction Functions"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2171
#, no-c-format
msgid ""
"These functions restrict the temporal value with respect to a value extent "
"or a time extent."
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2175
#, no-c-format
msgid "atValue"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2176
#, no-c-format
msgid "Restrict to a value"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2177
#, no-c-format
msgid "atValue(ttype, base): ttype"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2178
#, no-c-format
msgid ""
"SELECT atValue(tint '[1@2012-01-01, 1@2012-01-15)', 1);\n"
"-- \"[1@2012-01-01, 1@2012-01-15)\"\n"
"SELECT asText(atValue(tgeompoint '[Point(0 0 0)@2012-01-01, Point(2 2 "
"2)@2012-01-03)',\n"
"        'Point(1 1 1)'));\n"
"-- \"{[POINT Z (1 1 1)@2012-01-02]}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2182
#, no-c-format
msgid "atValues"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2183
#, no-c-format
msgid "Restrict to an array of values"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2184
#, no-c-format
msgid "atValues(ttype, base[]): ttype"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2185
#, no-c-format
msgid ""
"SELECT atValues(tfloat '[1@2012-01-01, 4@2012-01-4)', ARRAY[1, 3, 5]);\n"
"-- \"{[1@2012-01-01], [3@2012-01-03]}\"\n"
"SELECT asText(atValues(tgeompoint '[Point(0 0)@2012-01-01, Point(2 "
"2)@2012-01-03)',\n"
"        ARRAY[geometry 'Point(0 0)', 'Point(1 1)']));\n"
"-- \"{[POINT(0 0)@2012-01-01 00:00:00+00], [POINT(1 1)@2012-01-02 "
"00:00:00+00]}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2189
#, no-c-format
msgid "atRange"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2190
#, no-c-format
msgid "Restrict to a range"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2191
#, no-c-format
msgid "atRange(tnumber, numrange): ttype"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2192
#, no-c-format
msgid ""
"SELECT atRange(tfloat '[1@2012-01-01, 4@2012-01-4)', floatrange '[1,3]');\n"
"-- \"[1@2012-01-01, 3@2012-01-03]\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2196
#, no-c-format
msgid "atRanges"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2197
#, no-c-format
msgid "Restrict to an array of ranges"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2198
#, no-c-format
msgid "atRanges(tnumber, numrange[]): ttype"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2199
#, no-c-format
msgid ""
"SELECT atRanges(tfloat '[1@2012-01-01, 5@2012-01-05)', \n"
"        ARRAY[floatrange '[1,2]', '[3,4]']);\n"
"-- \"{[1@2012-01-01, 2@2012-01-02],[3@2012-01-03, 4@2012-01-04]}\""
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2203
#, no-c-format
msgid "atMin"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2204
#, no-c-format
msgid "Restrict to the minimum value"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:2205
#, no-c-format
msgid "atMin(torder): torder"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:2206
#, no-c-format
msgid ""
"The function returns null if the minimum value only happens at exclusive "
"bounds."
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:2207
#, no-c-format
msgid ""
"SELECT atMin(tint '{1@2012-01-01, 2@2012-01-03, 1@2012-01-05}');\n"
"-- \"{1@2012-01-01, 1@2012-01-05}\"\n"
"SELECT atMin(tint '(1@2012-01-01, 3@2012-01-03]');\n"
"-- \"{(1@2012-01-01, 1@2012-01-03)}\"\n"
