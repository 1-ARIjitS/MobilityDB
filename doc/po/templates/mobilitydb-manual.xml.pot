#. Tag: title
#: mobilitydb-manual.xml:582 mobilitydb-manual.xml:1134
#: mobilitydb-manual.xml:2367 mobilitydb-manual.xml:3357
#: mobilitydb-manual.xml:3593 mobilitydb-manual.xml:4190
#, no-c-format
msgid "Comparison Operators"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:584
#, no-c-format
msgid ""
"The comparison operators (<varname>=</varname>, <varname>&lt;</varname>, and "
"so on) require that the left and right arguments be of the same type. "
"Excepted equality and inequality, the other comparison operators are not "
"useful in the real world but allow B-tree indexes to be constructed on time "
"types."
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:586
#, no-c-format
msgid "The comparison operators available for the time types are given next."
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:590 mobilitydb-manual.xml:1140
#: mobilitydb-manual.xml:2377
#, no-c-format
msgid "<varname>=</varname>"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:591
#, no-c-format
msgid "Are the time values equal?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:592
#, no-c-format
msgid "time = time"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:593
#, no-c-format
msgid ""
"SELECT period '[2012-01-01, 2012-01-04)' = period '[2012-01-01, "
"2012-01-04)';\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:597 mobilitydb-manual.xml:1147
#: mobilitydb-manual.xml:2384
#, no-c-format
msgid "&lt;&gt;"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:598
#, no-c-format
msgid "Are the time values different?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:599
#, no-c-format
msgid "time &lt;&gt; time"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:600
#, no-c-format
msgid ""
"SELECT period '[2012-01-01, 2012-01-04)' &lt;&gt; period '[2012-01-03, "
"2012-01-05)';\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:604 mobilitydb-manual.xml:1154
#: mobilitydb-manual.xml:2391
#, no-c-format
msgid "&lt;"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:605
#, no-c-format
msgid "Is the first time value less than the second one?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:606
#, no-c-format
msgid "time &lt; time"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:607
#, no-c-format
msgid ""
"SELECT timestampset '{2012-01-01, 2012-01-04}' &lt; timestampset "
"'{2012-01-01, 2012-01-05}';\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:611 mobilitydb-manual.xml:1161
#: mobilitydb-manual.xml:2398
#, no-c-format
msgid "&gt;"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:612
#, no-c-format
msgid "Is the first time value greater than the second one?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:613
#, no-c-format
msgid "time &gt; time"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:614
#, no-c-format
msgid ""
"SELECT period '[2012-01-03, 2012-01-04)' &gt; period '[2012-01-02, "
"2012-01-05)';\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:618 mobilitydb-manual.xml:1168
#: mobilitydb-manual.xml:2405
#, no-c-format
msgid "&lt;="
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:619
#, no-c-format
msgid "Is the first time value less than or equal to the second one?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:620
#, no-c-format
msgid "time &lt;= time"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:621
#, no-c-format
msgid ""
"SELECT periodset '{[2012-01-01, 2012-01-04)}' &lt;= \n"
"        periodset '{[2012-01-01, 2012-01-05), [2012-01-06, 2012-01-07)}';\n"
"-- true"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:625 mobilitydb-manual.xml:1175
#: mobilitydb-manual.xml:2412
#, no-c-format
msgid "&gt;="
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:626
#, no-c-format
msgid "Is the first time value greater than or equal to the second one?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:627
#, no-c-format
msgid "time &gt;= time"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:628
#, no-c-format
msgid ""
"SELECT period '[2012-01-03, 2012-01-05)' &gt;= period '[2012-01-03, "
"2012-01-04)';\n"
"-- true"
msgstr ""

#. Tag: title
#: mobilitydb-manual.xml:634 mobilitydb-manual.xml:1184
#: mobilitydb-manual.xml:3387 mobilitydb-manual.xml:3622
#, no-c-format
msgid "Set Operators"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:636
#, no-c-format
