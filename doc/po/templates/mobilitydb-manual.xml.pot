#. Tag: varname
#: mobilitydb-manual.xml:1949
#, no-c-format
msgid "intersectsPeriod"
msgstr ""

#. Tag: para
#: mobilitydb-manual.xml:1950
#, no-c-format
msgid "Does the temporal value intersect the period?"
msgstr ""

#. Tag: varname
#: mobilitydb-manual.xml:1951
#, no-c-format
msgid "intersectsPeriod(ttype, period): boolean"
msgstr ""

#. Tag: programlisting
#: mobilitydb-manual.xml:1952
#, no-c-format
msgid ""
"SELECT intersectsPeriod(tint '[1@2012-01-01, 1@2012-01-04)',\n"
"        period '[2012-01-01,2012-01-05)');\n"
