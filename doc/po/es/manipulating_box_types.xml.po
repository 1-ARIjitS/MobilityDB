#. Tag: title
#: manipulating_box_types.xml:171
#, no-c-format
msgid "Modification Functions"
msgstr "Funciones de modificación"

#. Tag: para
#: manipulating_box_types.xml:173
#, no-c-format
msgid ""
"The functions given next expand the bounding boxes on the value and the time"
" dimension or set the precision of the value dimension. These functions "
"raise an error if the corresponding dimension is not present."
msgstr ""
"Las funciones dadas a continuación expanden los cuadros delimitadores en la "
"dimensión de valor y de tiempo o establecen la precisión de la dimensión de "
"valor. Estas funciones generan un error si la dimensión correspondiente no "
"está presente."

#. Tag: varname
#: manipulating_box_types.xml:177
#, no-c-format
msgid "expandValue"
msgstr "expandValue"

#. Tag: para
#: manipulating_box_types.xml:178
#, no-c-format
msgid ""
"Expand the numeric value dimension of the bounding box by a float value"
msgstr ""
"Extender la dimensión de valor numérico del cuadro delimitador con un valor "
"flotante"

#. Tag: varname
#: manipulating_box_types.xml:179
#, no-c-format
msgid "expandValue(tbox, float): tbox"
msgstr "expandValue(tbox, float): tbox"

#. Tag: programlisting
#: manipulating_box_types.xml:180
#, no-c-format
msgid ""
"SELECT expandValue(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', 1);\n"
"-- \"TBOX((0,2012-01-01),(3,2012-01-03))\"\n"
"SELECT expandValue(tbox 'TBOX((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have value dimension"
msgstr ""
"SELECT expandValue(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', 1);\n"
"-- \"TBOX((0,2012-01-01),(3,2012-01-03))\"\n"
"SELECT expandValue(tbox 'TBOX((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have value dimension"

#. Tag: varname
#: manipulating_box_types.xml:184
#, no-c-format
msgid "expandSpatial"
msgstr "expandSpatial"

#. Tag: para
#: manipulating_box_types.xml:185
#, no-c-format
msgid ""
"Expand the spatial value dimension of the bounding box by a float value"
msgstr ""
"Extender la dimensión de valor espacial del cuadro delimitador con un valor "
"flotante"

#. Tag: varname
#: manipulating_box_types.xml:186
#, no-c-format
msgid "expandSpatial(stbox, float): stbox"
msgstr "expandSpatial(stbox, float): stbox"

#. Tag: programlisting
#: manipulating_box_types.xml:187
#, no-c-format
msgid ""
"SELECT expandSpatial(stbox 'STBOX ZT((1,1,1,2012-01-01),(2,2,2,2012-01-03))', 1);\n"
"-- \"STBOX ZT((0,0,0,2012-01-01),(3,3,3,2012-01-03))\"\n"
"SELECT expandSpatial(stbox 'STBOX T((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have XY dimension"
msgstr ""
"SELECT expandSpatial(stbox 'STBOX ZT((1,1,1,2012-01-01),(2,2,2,2012-01-03))', 1);\n"
"-- \"STBOX ZT((0,0,0,2012-01-01),(3,3,3,2012-01-03))\"\n"
"SELECT expandSpatial(stbox 'STBOX T((,2012-01-01),(,2012-01-03))', 1);\n"
"-- The box must have XY dimension"

#. Tag: varname
#: manipulating_box_types.xml:191
#, no-c-format
msgid "expandTemporal"
msgstr "expandTemporal"

#. Tag: para
#: manipulating_box_types.xml:192
#, no-c-format
msgid "Expand the temporal dimension of the bounding box by a time interval"
msgstr ""
"Extender la dimensión temporal del cuadro delimitador con un intervalo de "
"tiempo"

#. Tag: varname
#: manipulating_box_types.xml:193
#, no-c-format
msgid "expandTemporal({tbox, stbox}, interval): {tbox, stbox}"
msgstr "expandTemporal({tbox, stbox}, interval): {tbox, stbox}"

#. Tag: programlisting
#: manipulating_box_types.xml:194
#, no-c-format
msgid ""
"SELECT expandTemporal(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', interval '1 day');\n"
"-- \"TBOX((1,2011-12-31),(2,2012-01-04))\"\n"
"SELECT expandTemporal(stbox 'STBOX ZT((1,1,1,2012-01-01),(2,2,2,2012-01-03))',\n"
"        interval '1 day');\n"
"-- \"STBOX ZT((1,1,1,2011-12-31),(2,2,2,2012-01-04))\""
msgstr ""
"SELECT expandTemporal(tbox 'TBOX((1,2012-01-01),(2,2012-01-03))', interval '1 day');\n"
"-- \"TBOX((1,2011-12-31),(2,2012-01-04))\"\n"
"SELECT expandTemporal(stbox 'STBOX ZT((1,1,1,2012-01-01),(2,2,2,2012-01-03))',\n"
"        interval '1 day');\n"
"-- \"STBOX ZT((1,1,1,2011-12-31),(2,2,2,2012-01-04))\""

#. Tag: varname
#: manipulating_box_types.xml:198
#, no-c-format
msgid "setPrecision"
msgstr "setPrecision"

#. Tag: para
#: manipulating_box_types.xml:199
#, no-c-format
msgid ""
"Round the value or the coordinates of the bounding box to a number of "
"decimal places"
msgstr ""
"Redondear el valor o las coordenadas del cuadro delimitador a un número de "
"decimales"

#. Tag: varname
#: manipulating_box_types.xml:200
#, no-c-format
msgid "setPrecision({tbox, stbox}, int): {tbox, stbox}"
msgstr "setPrecision({tbox, stbox}, int): {tbox, stbox}"

#. Tag: programlisting
#: manipulating_box_types.xml:201
#, no-c-format
msgid ""
"SELECT setPrecision(tbox 'TBOX((1.12345, 2000-01-01), (2.12345, 2000-01-02))', 2);\n"
"-- \"TBOX((1.12,2000-01-01),(2.12,2000-01-02))\"\n"
"SELECT setPrecision(stbox 'STBOX T((1.12345, 1.12345, 2000-01-01),\n"
"        (2.12345, 2.12345, 2000-01-02))', 2);\n"
"-- \"STBOX T((1.12,1.12,2000-01-01),(2.12,2.12,2000-01-02))\""
msgstr ""
"SELECT setPrecision(tbox 'TBOX((1.12345, 2000-01-01), (2.12345, 2000-01-02))', 2);\n"
"-- \"TBOX((1.12,2000-01-01),(2.12,2000-01-02))\"\n"
"SELECT setPrecision(stbox 'STBOX T((1.12345, 1.12345, 2000-01-01),\n"
"        (2.12345, 2.12345, 2000-01-02))', 2);\n"
"-- \"STBOX T((1.12,1.12,2000-01-01),(2.12,2.12,2000-01-02))\""

#. Tag: title
#: manipulating_box_types.xml:208
#, no-c-format
msgid "Spatial Reference System Functions"
msgstr "Funciones del sistema de referencia espacial"

#. Tag: varname
#: manipulating_box_types.xml:212
#, no-c-format
msgid "SRID"
msgstr "SRID"

#. Tag: para
#: manipulating_box_types.xml:213
#, no-c-format
msgid "Get the spatial reference identifier"
msgstr "Obtener el identificador de referencia espacial"

#. Tag: varname
#: manipulating_box_types.xml:214
#, no-c-format
msgid "SRID(stbox): int"
msgstr "SRID(stbox): int"

#. Tag: programlisting
#: manipulating_box_types.xml:215
#, no-c-format
msgid ""
"SELECT SRID(stbox 'STBOX ZT((1.0, 2.0, 3.0, 2000-01-01), (4.0, 5.0, 6.0, 2000-01-02))');\n"
"-- 0\n"
"SELECT SRID(stbox 'SRID=5676;STBOX T((1.0, 2.0, 2000-01-01), (4.0, 5.0, 2000-01-02))');\n"
"-- 5676\n"
"SELECT SRID(geodstbox 'GEODSTBOX T((, , 2000-01-01), (, , 2000-01-02))');\n"
"-- 4326"
msgstr ""
"SELECT SRID(stbox 'STBOX ZT((1.0, 2.0, 3.0, 2000-01-01), (4.0, 5.0, 6.0, 2000-01-02))');\n"
"-- 0\n"
"SELECT SRID(stbox 'SRID=5676;STBOX T((1.0, 2.0, 2000-01-01), (4.0, 5.0, 2000-01-02))');\n"
"-- 5676\n"
"SELECT SRID(geodstbox 'GEODSTBOX T((, , 2000-01-01), (, , 2000-01-02))');\n"
"-- 4326"

#. Tag: varname
#: manipulating_box_types.xml:219
#, no-c-format
msgid "setSRID"
msgstr "setSRID"

#. Tag: para
#: manipulating_box_types.xml:220
#, no-c-format
msgid "Set the spatial reference identifier"
msgstr "Especificar el identificador de referencia espacial"

#. Tag: varname
#: manipulating_box_types.xml:221
#, no-c-format
