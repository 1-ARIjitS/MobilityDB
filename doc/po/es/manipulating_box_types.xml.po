msgstr "¿Es el primer cuadro delimitador mayor o igual que el segundo?"

#. Tag: varname
#: manipulating_box_types.xml:278
#, no-c-format
msgid "{tbox, stbox} &gt;= {tbox, stbox}: boolean"
msgstr "{tbox, stbox} &gt;= {tbox, stbox}: boolean"

#. Tag: programlisting
#: manipulating_box_types.xml:279
#, no-c-format
msgid ""
"SELECT tbox 'TBOX((1, 2012-01-01), (1, 2012-01-04))' &gt;=\n"
"tbox 'TBOX((2, 2012-01-03), (2, 2012-01-05))'\n"
